class fibonaccic{
   public static void main(String args[])
   {
       int n=10,t1=0,t2=1;
       System.out.println("enter" + n + "integers");
       for(int i=0;i<=10;i++)
       {
          System.out.println(t1 + "+");
          int sum=t1+t2;
          t1=t2;
          t2=sum;
       }
   }
}
